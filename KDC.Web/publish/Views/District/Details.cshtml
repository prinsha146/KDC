@model KDC.DL.Models.MS_District

@{
    ViewBag.Title = "Details";
}


<section class="content-header">
    <h1>
        District Details
    </h1>
    <ol class="breadcrumb">
        <li class="list"><a href="@Url.Action("Index", "District")"><i class="fa fa-arrow-left"></i> List </a></li>
    </ol>
</section>

<section class="content">
    <div class="box">
        <div class="box-body">
            <div class="form-horizontal">

                <div class="form-group col-sm-12">
                    <div class="col-sm-2">
                        @Html.LabelFor(model => model.DistrictName, "District Name", new { @class = "control-label" })
                    </div>
                    <div class="col-sm-3">
                        @Html.TextBoxFor(model => model.DistrictName, new { @class = "form-control", @readonly = "readonly" })
                        @Html.ValidationMessageFor(model => model.DistrictName, "", new { @class = "text-danger" })
                    </div>
                </div>
            </div>
        </div>
    </div>
</section>
